						<script>
							var customselect = document.getElementsByClassName("custom-select-sm");
							var myHeaders = new Headers();
							myHeaders.append("Content-Type", "application/json")
							//baseUrl = "http://localhost:3000";
							baseUrl = "http://revoedubook.com";
							var url = baseUrl + "/customselect"
							var setPreferences = function () {
								console.log(customselect[0].value)
								var selectData = []
								for (i = 0; i < customselect.length; i++) {
									selectData.push(customselect[i].value)
									console.log(selectData)
								}
								var data = { data: selectData }
								document.cookie = selectData;

								var fetchData = {
									method: 'POST',
									body: JSON.stringify(data),
									headers: myHeaders,

								}


								fetch(url, fetchData)
									.then(function (response) {
										response.json().then(data => {
											console.log("data", data)
										var	html = '';

										for (var i=0;i<data.length; i++) {
											html += '<div class="col-md-6 col-sm-6 col-lg-3">';
											html += '<div class="subject_box" >';
											html += '<a href="#/">';
											html += '<div class="subject_icon" data-value =' + data.id + ' >';
											html += '<i class="fas fa-at"></i>';
											html += '</div><br>';
											html += '<h5>' + data.name + '</h5>';
											html += '</a>';
											html += '</div>';
											html += '</div>';
										}
										document.getElementById("current_topic").innerHTML = html;
										})
									})
							}
							for (var i = 0; i < customselect.length; i++) {
								customselect[i].addEventListener('change', setPreferences, false);
							}
						</script>




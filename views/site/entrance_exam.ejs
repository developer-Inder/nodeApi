<%- include ../sitelayouts/header.ejs %>

    <div class="main_video">
      <div class="overlay"></div>
      <video playsinline="playsinline" autoplay="autoplay" muted="muted" loop="loop">
        <source src="../video/ADRA Animated Short- Education.mp4" type="video/mp4">
          <source src="../video/mov_bbb.ogg" type="video/ogg">
      </video>
      <div class="container h-100">
        <div class="d-flex text-center h-100">
          <div class="my-auto w-100 text-white">
            <h1 class="display-3 text-uppercase font-weight-bold">Entrance <b class="main_color">Exam</b></h1>
          </div>
        </div>
      </div>
    </div>

    <div class="container-fluid my-4">
    	<div class="row">
    		<% if (locals.data.exams_list) { %>
    		   <% locals.data.exams_list.forEach(function(exam){%>
	           
    		<div class="col-md-5 col-lg-3 col-sm-5 d-none d-sm-block category_img">
    			<a href="#"><img class="img-fluid" src="../images/2.jpg" alt=""></a>
    		</div>
		    <div class="col-md-7 col-lg-9 col-sm-7">
				<div class="main_title">		
					<a href="engineering_entrancexam"><%= exam.exam_type %></a>
				</div>	
				<div id="owl-demo" class="text-center">     
				   <% if (exam.list) { %>
			           <% exam.list.forEach(function(cat){%>
					<div class="item">
						<div class="section_box">
						  <a href="/exam_detail/?exam_id=<%= cat.id %>"><div class="product_icon">
							<i class="fas fa-user-graduate"></i>
						  </div><br>
						  <h5><%= cat.exam_name %></h5></a>
						</div>
					</div>
					 <% }) %>
			          <% } %>	
				</div>
			</div>
			<% }) %>
	       	<% } %>	
		</div>
	</div>
	
<%- include ../sitelayouts/footer.ejs %>
    <script type="text/javascript">
    	console.log(<%= locals.data.exams_list %>)
    </script>>